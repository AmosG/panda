<% if (!query) { %>

    <p>Please enter a search query.</p>

<% } else if (total_count == 0) { %>

    <p>No rows matching query "<%= query %>" found in dataset <strong><%= dataset.name %></strong>.</p>

    <p>Would you like to <a href="#search/<%= query %>">search all datasets instead?</a></p>

<% } else { %>

    <%= pager %>

    <%= dataset_block %>

    <%= pager %>

<% } %>

