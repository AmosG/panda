<li>
    <strong><a href="/api/1.0/related_upload/<%= upload.id %>/download/"><%= upload.original_filename %></a></strong> (<strong><%= upload.size %> bytes</strong>)
    uploaded by <a href="mailto:<%= upload.creator.email %>" data-original-title="<%= upload.creator.email %>" rel="tooltip" ><%= upload.creator.first_name %><% if (upload.creator.first_name && upload.creator.last_name) { %> <% } %><%= upload.creator.last_name %></a>
    
    <a title="File history" data-content="Uploaded on <strong><%= moment(upload.creation_date, "YYYY-MM-DDTHH:mm:ss").toLocalTimezone().format("MMMM D, YYYY @ h:mm a") %></strong></p>" data-placement="top" href="javascript: return false;" rel="popover"><i class="icon-calendar"></i></a>
    
    <a alt="Delete" href="javascript: return false;" class="delete" data-uri="<%= upload.resource_uri %>"><i class="icon-trash"></i></a>
</li>
