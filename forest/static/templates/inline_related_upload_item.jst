<li>
    <strong><a href="/api/1.0/related_upload/<%= upload.id %>/download/"><%= upload.original_filename %></a></strong> (<strong><%= upload.size %> bytes</strong>)

    <a title="File history" data-content="<p>Uploaded by <strong><%= upload.creator.first_name %> <%= upload.creator.last_name %></strong> &lt;<a href=&quot;mailto:<%= upload.creator.email %>&quot;><%= upload.creator.email %></a>&gt;<br />on <strong><%= moment(upload.creation_date, "YYYY-MM-DDTHH:mm:ss").toLocalTimezone().format("MMMM D, YYYY @ h:mm a") %></strong></p>" data-placement="top" href="javascript: return false;" rel="popover"><i class="icon-search"></i></a>
    
    <a alt="Delete" href="javascript: return false;" class="delete" data-uri="<%= upload.resource_uri %>"><i class="icon-trash"></i></a>
</li>
